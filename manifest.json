{
    "manifest_version": 3,
    "name": "Just Search",
    "description": "This is a very simple new TAB extension for Chrome. The purpose is to replace Chrome's default tabs.",
    "version": "0.2",
    "icons": {
        "128": "assets/icons/128x128/just-search.png",
        "256": "assets/icons/256x256/just-search.png",
        "512": "assets/icons/512x512/just-search.png"
    },
    "action": {
        "default_popup": "src/introduce.html",
        "default_icon": "assets/icons/256x256/just-search.png"
    },
    "chrome_url_overrides": {
        "newtab": "src/index.html"
    }
}